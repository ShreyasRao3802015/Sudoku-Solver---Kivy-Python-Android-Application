WindowManager:
    Menu:
    Solver:
    Htps:
    Suggestion:

<Suggestion>:
    name: 'Suggestion'
    FloatLayout:
        Label:
            text:'Suggestions?'
            color: 0.3656,0.841,1,1
            font_size:self.width/10
            pos_hint:{'x':-0.2,'y':0.35}

        Button:
            font_size : root.width/20
            text: 'Home'
            color: 0.3656,0.841,1,1
            background_color: 1,1,1,1
            size_hint: 0.25,0.08
            pos_hint: {'x':0.7, 'y':0.8}
            on_release:
                app.root.current = 'menu'
                root.manager.transition.direction = 'right'

        ScrollView:
            pos_hint:{'x':0.05,'y':-0.4}
            Label:
                text:'Any suggestions are gladly welcome!\n\nContact :\nshreyas.rao380@gmail.com\n7734848262'
                font_size : self.width/15
                text_size : self.width,None
                size_hint_y: None
                height: self.texture_size[1]


<Htps>:
    name: 'Htps'
    FloatLayout:
        Label:
            text:'How to play \nSudoku?'
            color: 0.3656,0.841,1,1
            font_size:self.width/10
            pos_hint:{'x':-0.2,'y':0.35}

        ScrollView:

            pos_hint:{'x':0.05,'y':-0.3}
            Label:
                text:'->Sudoku is played on a grid of 9 x 9 spaces.\n\n->Within the rows and columns are 9 “squares” (made up of 3 x 3 spaces).\n\n-> Each row, column and square (9 spaces each) needs to be filled out with the numbers 1-9, without repeating any numbers within the row, column or square.'
                font_size : self.width/15
                text_size : self.width*0.9,None
                size_hint_y: None
                height: self.texture_size[1]

        Button:
            font_size : root.width/20
            text: 'Home'
            color: 0.3656,0.841,1,1
            background_color: 1,1,1,1
            size_hint: 0.25,0.1
            pos_hint: {'x':0.7, 'y':0.8}
            on_release:
                app.root.current = 'menu'
                root.manager.transition.direction = 'right'




<invalid_popup>:
    popup_close: popup_close
    Label:
        text: "Invalid Input!!"
        font_size: root.width/10
        size_hint:0.3,0.3
        pos_hint: { 'x':0.35, 'top': 1}

    Button:
        id: popup_close
        text: 'Close'
        font_size: root.width/10
        size_hint:0.3,0.3
        pos_hint: { 'x':0.35, 'y': 0}



<Menu>:
    name: 'menu'

    Label:
        text: "Sudoku Solver"
        color: 1,0,1,1
        font_size: root.width/10
        size_hint:1,0.1
        pos_hint: { 'x':0, 'top': 1}

    Button:
        text: "Let's Start!"
        color: 1,1,0,1
        font_size: root.width/10
        size_hint:0.8,0.1
        background_color: 0,0,0,1
        pos_hint: { 'x':0.1, 'y': 0.6}
        on_release:
            app.root.current = 'solver'
            root.manager.transition.direction = 'left'


    Button:
        text: "How to play \n Sudoku?"
        color: 1,1,0,1
        font_size: self.width/10
        size_hint:0.8,0.15
        background_color: 0,0,0,1
        pos_hint: { 'x':0.1, 'y': 0.4}
        on_release:
            app.root.current = 'Htps'
            root.manager.transition.direction = 'left'

    Button:
        text: "Suggestions?"
        color: 1,1,0,1
        font_size: self.width/10
        size_hint:0.8,0.15
        background_color: 0,0,0,1

        pos_hint: { 'x':0.1, 'y': 0.2}
        on_release:
            app.root.current = 'Suggestion'
            root.manager.transition.direction = 'left'

